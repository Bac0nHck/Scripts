    if GPE or Input.KeyCode ~= Enum.KeyCode.Zero then return end

    NotifyModule:Notify("copied invite link to clipboard")
    setclipboard("https://discord.com/invite/FRsmP9knVc")
    Connection:Disconnect()
end)
